{
  "compilerOptions": {
    "sourceMap": true,
    "lib": ["es5", "es6", "dom", "es2015", "esnext.asynciterable"],
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  }
}
